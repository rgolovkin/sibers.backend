{
  "name": "sibers-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch ./ --ext js,json --exec \"clear && node server.js\"",
    "docker": "docker-compose up -d",
    "migration:generate": "npx sequelize-cli migration:generate --name $npm_config_name && node fix-migration.js",
    "migration": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/jwt": "^9.1.0",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/view": "^11.1.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.6.1",
    "fastify": "^5.4.0",
    "fastify-jwt": "^4.2.0",
    "fastify-plugin": "^5.0.1",
    "fastify-static": "^4.7.0",
    "fluent-json-schema": "^6.0.0",
    "handlebars": "^4.7.8",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "point-of-view": "^6.3.0",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3"
  }
}
